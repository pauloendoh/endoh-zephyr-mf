#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/cli/bin/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/cli/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/cli/bin/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/cli/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules/@rspack/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/@rspack+cli@1.2.8_@rspack+core@1.2.8_@types+express@4.17.25_webpack@5.103.0/node_modules:/home/pauloendoh/programming_wsl/_zephyr/endoh-zephyr-mf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@rspack/cli/bin/rspack.js" "$@"
else
  exec node  "$basedir/../@rspack/cli/bin/rspack.js" "$@"
fi
